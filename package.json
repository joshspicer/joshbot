{
  "name": "joshbot",
  "displayName": "Josh Bot",
  "description": "A dedicated coding assistant great at complex tasks",
  "version": "1.0.0",
  "publisher": "spcr-test",
  "license": "MIT",
  "engines": {
    "vscode": "^1.102.0"
  },
  "enabledApiProposals": [
    "chatSessionsProvider",
    "chatParticipantAdditions",
    "chatParticipantPrivate"
  ],
  "main": "./out/extension.js",
  "categories": [
    "Other"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "activationEvents": [
    "onChatSession:josh-bot"
  ],
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "contributes": {
    "chatSessions": [
      {
        "id": "josh-bot",
        "name": "joshbot",
        "displayName": "Josh Bot",
        "description": "A dedicated coding assistant great at complex tasks",
        "when": "true"
      }
    ],
    "commands": [
      {
        "command": "joshbot.hello",
        "title": "Hello",
        "category": "JoshBot"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "deploy": "vsce publish",
    "postinstall": "npm run downloadapi",
    "downloadapi": "(echo 'fetching api disabled' && exit 0) || npx vscode-dts dev"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/node": "16.x",
    "@vscode/vsce": "^3.6.0",
    "typescript": "^5.8.0",
    "@vscode/dts": "^0.4.1"
  },
  "dependencies": {
    
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshspicer/joshbot.git"
  }
}
